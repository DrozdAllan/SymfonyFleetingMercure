{% extends "base.html.twig" %}

{% block body %}
	<div class="container">


		<h1>
			Ajouter une image
		</h1>
		<div class="row">

			<div class="col">

				{{ form_start(formView) }}

				{{ form_row(formView.image) }}

				<button type="submit" class="btn btn-danger">
					Ajouter
				</button>
				{{ form_end(formView) }}

			</div>
		</div>
	</div>

{% endblock %}
